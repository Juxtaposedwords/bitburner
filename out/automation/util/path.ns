/** @param {import("../../..").NS } ns */

import { dfs } from  "/automation/lib/scan.ns"

export async function main(ns) {
    const toFind = ns.args[0];
    const path = [];
    if (await dfs(ns, path, "home", toFind)) {
      ns.tprint(path);
      return;
    }
    ns.tprint(`${toFind} not found.`);
  }
  
